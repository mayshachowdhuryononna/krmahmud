    "def digi_Root(n): \n",
    "   \n",
    "    \n",
    "    sum = 0\n",
    "    \n",
    "    \n",
    "    while(n > 0 or sum > 9): \n",
    "        if(n == 0): \n",
    "            n = sum\n",
    "            sum = 0\n",
    "          \n",
    "        sum += n % 10\n",
    "        n /= 10 \n",
    "      \n",
    "    return sum\n",
